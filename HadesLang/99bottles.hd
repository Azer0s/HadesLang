for[num i in range:[99,3]]
	out:$i + ' bottles of beer on the wall, ' + $i + ' bottles of beer.'
	j as num = $i - 1
	out:'Take one down and pass it around, ' + $j + ' bottles of beer on the wall.\n'
end

