c as num closed = fib:[1]
out:c
//Testing
func fib[num a]
	b as num closed = 1
	while[true]
		out:a
		out:b

		a = $a + $b
		b = $a + $b

		if[$a bigger 10000]
			put a
		end
	end
end

func fizzbuzz[]
	a as bit closed
	b as bit closed
	for[num i in range:[0,100]]
		a = ($i % 3) is 0
		b = ($i % 5) is 0

		if[$a and $b]
			out:'Fizzbuzz ' + $i
		end
		else
			if[$a]
				out:'Fizz ' + $i
			end
			else
				if[$b]
					out:'Buzz ' + $i
				end
			end
		end
	end
end